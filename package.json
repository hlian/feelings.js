{
  "name": "feelings.js",
  "version": "^1.0.0",
  "description": "formerly feelings.hs",
  "main": "index.js",
  "repository": "https://github.com/hlian/feelings.js",
  "author": "hao",
  "license": "MIT",
  "scripts": {
    "_prettier": "prettier \"./**/*.{js,jsx,ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit -p .",
    "test": "concurrently -n lint,tsc 'yarn jest' 'yarn typecheck'",
    "fix": "yarn _prettier --write",
    "execstart": "nodemon",
    "dev": "nodemon --watch"
  },
  "dependencies": {
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "luxon": "^1.17.3",
    "mustache": "^3.1.0",
    "nodemon": "^1.19.2",
    "onchange": "^6.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.6.3",
    "write-file-atomic": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.0.50",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/luxon": "^1.15.2",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.11.4",
    "@types/write-file-atomic": "^2.1.2",
    "concurrently": "^4.1.2",
    "fuse-box": "^3.2.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/etc/preprocessor.js"
    }
  }
}
