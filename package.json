{
  "name": "feelings.js",
  "version": "^1.0.0",
  "description": "formerly feelings.hs",
  "main": "index.js",
  "repository": "https://github.com/hlian/feelings.js",
  "author": "hao",
  "license": "MIT",
  "scripts": {
    "test": "concurrently -n lint,tsc 'yarn jest' 'yarn tsc'",
    "prettier-watch": "onchange 'fuse.js' 'server/**/*.ts' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "luxon": "^1.3.0",
    "mustache": "^2.3.0",
    "onchange": "^4.0.0",
    "typescript": "^2.9.2",
    "write-file-atomic": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.28",
    "@types/luxon": "^0.5.3",
    "@types/mustache": "^0.8.30",
    "@types/node": "^10.3.4",
    "@types/write-file-atomic": "^2.1.1",
    "concurrently": "^3.6.0",
    "fuse-box": "^3.2.2",
    "jest": "^23.1.0",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/etc/preprocessor.js"
    }
  }
}
