{
  "name": "feelings.js",
  "version": "^1.0.0",
  "description": "formerly feelings.hs",
  "main": "index.js",
  "repository": "https://github.com/hlian/feelings.js",
  "author": "hao",
  "license": "MIT",
  "scripts": {
    "test": "concurrently -n lint,tsc 'yarn jest' 'yarn tsc'",
    "prettier-watch": "onchange 'fuse.js' 'server/**/*.ts' -- prettier --write {{changed}}"
  },
  "dependencies": {
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "luxon": "^1.12.0",
    "mustache": "^3.0.1",
    "onchange": "^5.2.0",
    "typescript": "^3.4.1",
    "write-file-atomic": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.2.2",
    "@types/koa-router": "^7.0.40",
    "@types/luxon": "^1.12.0",
    "@types/mustache": "^0.8.32",
    "@types/node": "^12.0.5",
    "@types/write-file-atomic": "^2.1.1",
    "concurrently": "^4.1.0",
    "fuse-box": "^3.2.2",
    "jest": "^24.6.0",
    "prettier": "^1.13.5",
    "tslint": "^5.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/etc/preprocessor.js"
    }
  }
}
